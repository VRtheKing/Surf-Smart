{
    "manifest_version":3,
    "version": "0.0.24",
    "name": "Surf Smart",
    "action": {
        "default_popup": "index.html"
    },
    "icons": {
      "128": "logo.png"
    },
    "omnibox": {"keyword": "qs"},
    "permissions": [
        "scripting",
        "tabs",
        "tabs",
        "tabGroups"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["contentScript.js"]
        }
      ],
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "options.html",
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+K",
                "mac": "MacCtrl+Command+K"
            }
        }
    }
}